package wattsup{	import flash.display.Sprite;	import flash.events.MouseEvent;	import com.chootka.utils.display.GlowOutlineRect;	import com.dixond.events.CustomEvent;	import com.dixond.logging.*;	import com.dixond.utils.Textd;	import com.dixond.utils.Util;	import com.greensock.*;	import com.greensock.easing.*;	import com.greensock.events.*;	import com.greensock.loading.*;	import com.greensock.loading.display.*;			public class Screen extends Sprite	{		public var _log:Logger;		public var _images:Object;		public var _sounds:Object;		public var _tm:MLTextManager;				public function Screen(images:Object,sounds:Object,tm:MLTextManager)		{			_images = images;			_sounds = sounds;			_tm = tm;						initLog();		}				public function initLog() {			// start off with default to DEBUG. If config file differs, it will overwrite this setting			_log = Logger.getLog();		}				public function onButtonUp($e:CustomEvent):void {			dispatchEvent( $e );		}				public function removeFromLayer(a:Array) {			for (var i=0; i<a.length; i++) {				this.removeChild(a[i]);			}		}				public function addToLayer(a:Array) {			// repopulate the layer			for (var i=0; i<a.length; i++) {				this.addChild(a[i]);			}		}				/*public function destroy() {					}*/	}}