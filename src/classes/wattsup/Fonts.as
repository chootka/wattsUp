package wattsup{		/*	*	Document class for 'fonts.swf' file.	*	registers all fonts embeded in the library of th fonts.fla file.	*		*	*/		import flash.display.MovieClip;	import flash.text.Font;	import flash.text.TextField;	import flash.text.TextFormat;		public class Fonts extends MovieClip {		private var _embeddedFonts:Array;				public function Fonts(){ 			// note these are the Class names, not the font names to be used in text format code...			Font.registerFont(Helvetica);			Font.registerFont(HelveticaMed);			_embeddedFonts = Font.enumerateFonts(false);			_embeddedFonts.sortOn("fontName", Array.CASEINSENSITIVE);			listFonts();		}				public function listFonts() {			// list the fonts showing both the class name and the fontName so we can see the correct terms to use			trace("----- fonts loaded Class:FontName-----");			for each (var f:Object in _embeddedFonts) {				trace("\tfontClass: " + f + " fontName: " + f.fontName);			}			trace("----- end font classes -----");		}				public function getFontList() {			var str:String = "";			for each (var f:Object in _embeddedFonts) {				str += ("\tfontClass: " + f + " || fontName: " + f.fontName + "\n");			}			return str;		}					} //end class}//end package